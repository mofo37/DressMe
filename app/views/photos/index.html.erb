<p id="notice"><%= notice %></p>

<h1>Photos</h1>

<table class='table'>
  <thead>
    <tr>
      <th>Photo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @photos.each do |photo| %>
      <tr>
        <td><%= link_to image_tag(photo.url), photo.url %></td>
      </tr>
    <% end %>
  </tbody>
</table>
