<h1><%= @tag.name %></h1>

<% @items.in_groups_of(4).each do |group| %>
  <div class='row mb-4'>
    <% group.each do |image, item| %>
      <div class='col'>
        <% if item.present? %>
          <%= link_to image_tag(image), item %>
        <% end %>
      </div>
    <% end %>
  </div>
<% end %>
